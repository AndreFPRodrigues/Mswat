<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mswat.core.activityManager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <service
            android:name="mswat.core.activityManager.HierarchicalService"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE"
            android:settingsActivity="mswat.core.activityManager.ServicePreferences" >
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/serviceconfig" />
        </service>

        <activity
            android:name="mswat.core.activityManager.ServicePreferences"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.accessibilityservice.SERVICE_SETTINGS" />
            </intent-filter>
        </activity>

        <receiver
            android:name="mswat.touch.TPRTab2"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_tpr" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="mswat.touch.TPRNexusS"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_tpr" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="tutorials.controlInterface.TouchController"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="mswat.caseStudy.controllers.autonav.AutoNav"
            android:enabled="true" >
            <intent-filter android:priority="99999" >
                <action android:name="mswat_autoNavStart" />
                <action android:name="contentUpdate" />
                <action android:name="mswat_keyboard" />
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="tutorials.adapt.TouchAdapter"
            android:enabled="true" >
            <intent-filter>
                <action android:name="monitor" />
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="mswat.core.logger.Logger"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="tutorials.receivers.NoteTutorial"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="tutorials.receivers.ContentTutorial"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="mswat.caseStudy.controllers.autonav.AutoNavKeyboard"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
                <action android:name="mswat_keyboard" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="mswat.caseStudy.multiScreenReader.ScreenReader"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init_screenReader" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="tutorials.receivers.IOTutorial"
            android:enabled="true" >
            <intent-filter>
                <action android:name="mswat_init" />
                <action android:name="mswat_stop" />
            </intent-filter>
        </receiver>

        <activity
            android:name="mswat.core.calibration.CalibrationActivity"
            android:label="@string/title_activity_calibration" >
        </activity>
        <activity
            android:name="mswat.core.calibration.CalibrationScreen"
            android:label="@string/title_activity_calibration" >
        </activity>
        <activity
            android:name="mswat.caseStudy.controllers.autonav.AutoNavSetup"
            android:label="@string/title_activity_calibration" >
        </activity>
        <activity
            android:name="mswat.core.macro.RunMacro"
            android:label="MacroSwat" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
    </application>

</manifest>